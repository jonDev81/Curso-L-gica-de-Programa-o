Algoritmo "Formatar_numeros"
Var
// Seção de Declarações das variáveis 
   a, b : inteiro
   x, y : real

Inicio
   a <- 586
   b <- 523584
   x <- 17.8
   y <- 1.893
   
   escreval(a:5)  // Reserva o número de espaços e alinha para a direita
   escreval(b:5)

   escreval(x:10:2)  //  Reserva o espaço com casas decimal
   escreval(y:6:2)

Fimalgoritmo