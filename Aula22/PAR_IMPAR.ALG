Algoritmo "par_impar"

Var
// Seção de Declarações das variáveis 
   numero : inteiro

Inicio
// Entrada de dados:
   escreva("Digite um número: ")
   leia(numero)
   
// Processamento:
   se(numero % 2 = 0) então
     escreval(numero," é par")
   senão
     escreval(numero," é impar")
   fimse


Fimalgoritmo