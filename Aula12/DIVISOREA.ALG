Algoritmo "Divisorea"
Var
// Seção de Declarações das variáveis 
   a ,b, divisao_int, resto: inteiro
   c, d, divisao: real

Inicio
// Processamento
   // Tipo inteiro
   a <- 5
   b <- 2
   
   divisao_int <- a \ b
   resto <- a MOD b
   // ou
   resto <- a % b
   
   // Tipo real
   c <- 5
   d <- 2
   divisao <- c / d
Fimalgoritmo